{
  "name": "lesspass-pure",
  "version": "3.1.1",
  "description": "pure LessPass component for LessPass with vuejs",
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "repository": "lesspass/pure",
  "scripts": {
    "dev": "npm install && webpack-dev-server --inline --hot --host 0.0.0.0",
    "build": "rm -rf dist && NODE_ENV=production webpack --display-error-details --progress --hide-modules",
    "test": "ava test --compilers js:babel-register",
    "prepublish": "npm test && npm run build"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "bootstrap": "4.0.0-alpha.5",
    "clipboard": "^1.5.15",
    "font-awesome": "^4.7.0",
    "hint.css": "^2.4.0",
    "jwt-decode": "^2.1.0",
    "lesspass": "^5.1.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.0.3",
    "vue-router": "^2.0.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.6",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "nock": "^9.0.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "style-loader": "^0.13.1",
    "unminified-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.5.1",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": false
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
