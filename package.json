{
  "name": "lesspass-pure",
  "version": "4.4.3",
  "description": "pure LessPass component for LessPass with vuejs",
  "license": "GPL-3.0",
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "repository": "lesspass/pure",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --display-error-details --progress --hide-modules",
    "dev": "webpack-dev-server --inline --hot --host 0.0.0.0",
    "prepublish": "npm test && npm run build",
    "test": "ava test --compilers js:babel-register"
  },
  "babel": {
    "comments": false,
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "awesomplete": "^1.1.1",
    "axios": "^0.15.2",
    "bootstrap": "4.0.0-alpha.6",
    "clipboard": "^1.5.15",
    "font-awesome": "^4.7.0",
    "hint.css": "^2.4.1",
    "jwt-decode": "^2.1.0",
    "lesspass": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.1",
    "vuex-persistedstate": "^1.1.0",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "json-loader": "^0.5.4",
    "nock": "^9.0.0",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "timekeeper": "^1.0.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
